Proceso calculadora
	definir numA, numB, sum, res, mul,div Como Real;
	
	Escribir "ingrese 2 numeros";
	leer numA,numB;
	
	sum=numA+numB;
	res=numA-numB;
	mul=numA*numB;
	div=numA/numB;
	
	Escribir "el resultado de la suma es: ", sum,", el de la resta es : ",res,", el de la multiplicación es: ",mul,", el de la división es: ",div;
	
	
	
FinProceso
