Proceso descuentos15Porcent
	definir totalCompra, descuento, totalFinal Como Real;
	
	Escribir "ingrese el total neto de su compra";
	leer totalCompra;
	descuento=(totalCompra*15)/100;
	totalFinal=(totalCompra-descuento);
	
	Escribir "lo que usted tendrá que pagar aplido el 15% de descuento es: ",totalFinal;
	
FinProceso
